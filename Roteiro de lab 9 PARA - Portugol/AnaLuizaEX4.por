//Descricao: numeros de 100 a 50 em ordem decrescente 

programa
 //inicio do programa 
{
	
inteiro i //declaracao da variavel
	funcao inicio()
 //inicio da funcao
	{
		para(i=100; i>=50;i--){ //inicio do para, com as condicoes 
		escreva(i,"\n")//mensagem exibida ao usuario 
		} //fim para 
	//fim funcao
	}
//fim programa 
}

